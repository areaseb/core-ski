{
    "name": "areaseb/core",
    "description": "Core Functionality CRM",
    "license": "MIT",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Giacomo Gasperini",
            "email": "giacomo.gasperini@areaseb.it",
            "homepage": "https://www.areaseb.it"
        }
    ],
