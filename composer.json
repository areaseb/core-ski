{
    "name": "areaseb/core-ski",
    "description": "Core Functionality CRM for ski school",
    "license": "MIT",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Davide Moscheni",
            "email": "assistenza@areaseb.it",
            "homepage": "https://www.areaseb.it"
        }
    ],
    "homepage": "https://github.com/areaseb/core-ski",
    "keywords": ["Laravel", "Core-ski"],
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Areaseb\\Core\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Areaseb\\Core\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Areaseb\\Core\\CoreServiceProvider"
            ],
            "aliases": {
                "Core": "Areaseb\\Core\\Facades\\Core"
            }
        }
    }
}
