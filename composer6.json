{
    "name": "areaseb/core",
    "description": "Core Functionality CRM",
    "license": "MIT",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Giacomo Gasperini",
            "email": "giacomo.gasperini@areaseb.it",
            "homepage": "https://www.areaseb.it"
        }
    ],
    "homepage": "https://github.com/areaseb/core",
    "keywords": ["Laravel", "Core"],
    "require": {
        "illuminate/support": "~6|~7",
        "barryvdh/laravel-snappy": "^0.4.8",
        "h4cc/wkhtmltopdf-amd64": "^0.12.4",
        "intervention/image": "^2.5",
        "laravel/helpers": "^1.2",
        "laravelcollective/html": "^6.2",
        "maatwebsite/excel": "^3.1",
        "spatie/laravel-permission": "^3.17",
        "doctrine/dbal": "2.12"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Areaseb\\Core\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Areaseb\\Core\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Areaseb\\Core\\CoreServiceProvider"
            ],
            "aliases": {
                "Core": "Areaseb\\Core\\Facades\\Core"
            }
        }
    }
}
